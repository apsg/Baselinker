{
    "name": "apsg/baselinker",
    "description": "Baselinker Integration",
    "license": "MIT",
    "authors": [
        {
            "name": "Szymon Gackowski",
            "email": "szymon@gackowski.edu.pl",
            "homepage": "https://gackowski.edu.pl"
        }
    ],
    "homepage": "https://github.com/apsg/Baselinker",
    "keywords": [
        "Laravel",
        "Baselinker"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5|~6",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Apsg\\Baselinker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apsg\\Baselinker\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apsg\\Baselinker\\BaselinkerServiceProvider"
            ],
            "aliases": {
                "Baselinker": "Apsg\\Baselinker\\Facades\\Baselinker"
            }
        }
    }
}
